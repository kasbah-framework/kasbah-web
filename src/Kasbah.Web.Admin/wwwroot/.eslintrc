{
  "parser"  : "babel-eslint",
  "extends" : [
  ],
  "env"     : {
    "browser" : true
  },
  "globals" : {
    "__DEV__"      : false,
    "__PROD__"     : false,
    "__DEBUG__"    : false,
    "__DEBUG_NEW_WINDOW__" : false,
    "API_URL"      : false
  },
  "rules": {
    "semi" : [2, "always"]
  }
}
